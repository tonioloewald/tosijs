{
  "name": "xinjs",
  "version": "0.8.3",
  "description": "path-based state management for web apps",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.iife.js",
      "default": "./dist/index.js"
    }
  },
  "homepage": "https://xinjs.net",
  "keywords": [
    "javascript",
    "typescript",
    "bun",
    "front-end",
    "back-end",
    "web-components",
    "css-variables"
  ],
  "scripts": {
    "start": "bun dev.ts",
    "format": "bun eslint src demo --fix && bun prettier --write .",
    "test": "bun test",
    "latest": "rm -rf node_modules && npm update && bun install",
    "free-port": "lsof -ti:8018 | xargs kill -9"
  },
  "author": "Tonio Loewald",
  "license": "BSD-3-CLAUSE",
  "devDependencies": {
    "@happy-dom/global-registrator": "^13.10.1",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "bun-types": "^1.2.5",
    "eslint": "^8.57.1",
    "marked": "^5.1.2",
    "parcel": "latest",
    "prettier": "^2.8.8",
    "typescript": "^5.6.2",
    "xinjs-ui": "^0.6.56"
  },
  "files": [
    "/cdn",
    "/dist",
    "/LICENSE",
    "/README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/xinjs.git"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "cdn",
      "public",
      "www",
      "**/*.js",
      "**/*.mjs",
      "**/*.d.ts"
    ]
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001701"
  }
}
