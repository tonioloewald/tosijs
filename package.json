{
  "name": "tosijs",
  "version": "1.0.7",
  "description": "path-based state management for web apps",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/module.js",
    "require": "./dist/main.js",
    "browser": "./dist/index.js",
    "default": "./dist/module.js",
    "types": "./dist/index.d.ts"
  },
  "homepage": "https://tosijs.net",
  "keywords": [
    "javascript",
    "typescript",
    "bun",
    "front-end",
    "back-end",
    "web-components",
    "css-variables"
  ],
  "scripts": {
    "start": "bun --watch dev.ts",
    "format": "bun eslint src demo --fix && bun prettier --write .",
    "test": "bun test",
    "latest": "rm -rf node_modules && bun update",
    "free-port": "lsof -ti:8018 | xargs kill -9"
  },
  "author": "Tonio Loewald",
  "license": "BSD-3-CLAUSE",
  "devDependencies": {
    "@happy-dom/global-registrator": "^18.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.21",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "bun-types": "^1.3.0",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.1",
    "marked": "^16.4.0",
    "prettier": "^2.8.8",
    "typescript": "^5.9.3",
    "tosijs": "file:.",
    "tosijs-ui": "^1.0.6"
  },
  "files": [
    "/dist",
    "/LICENSE",
    "/README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/tosijs.git"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "cdn",
      "public",
      "www",
      "**/*.js",
      "**/*.mjs",
      "**/*.d.ts"
    ]
  }
}
