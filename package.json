{
  "name": "xinjs",
  "version": "0.2.5",
  "description": "path-based state management for web apps",
  "main": "dist/index.js",
  "homepage": "https://xinjs.net",
  "keywords": [
    "javascript",
    "typescript",
    "bun",
    "front-end",
    "back-end"
  ],
  "scripts": {
    "start": "rm -rf www && mkdir -p www/assets && cp readme.md xinjs-logo.svg www/assets && cp -r demo/assets/* www/assets && bun parcel --dist-dir www",
    "lint": "bun ts-standard --fix src/*.ts",
    "make": "rm -rf dist && bun test && bun tsc && ./build.command",
    "pack": "bun lint && bun run make && npm pack --pack-destination ~",
    "test": "bun wiptest"
  },
  "author": "Tonio Loewald",
  "license": "BSD-3-CLAUSE",
  "devDependencies": {
    "babylonjs": "^5.31.2",
    "babylonjs-gui": "^5.32.1",
    "babylonjs-loaders": "^5.32.1",
    "babylonjs-materials": "^5.32.1",
    "bun-types": "^0.2.0",
    "marked": "^4.2.2",
    "parcel": "^2.8.3",
    "rollup": "^3.2.3",
    "ts-standard": "^12.0.1",
    "typescript": "^4.8.4",
    "uglify-js": "^3.1.7"
  },
  "files": [
    "/dist",
    "/LICENSE",
    "/README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/xinjs.git"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "cdn",
      "public",
      "www",
      "**/*.js",
      "**/*.mjs",
      "**/*.d.ts"
    ]
  },
  "targets": {
    "default": {
      "source": "demo/index.html",
      "distDir": "www",
      "outputFormat": "esmodule",
      "scopeHoist": false
    }
  }
}
