{
  "name": "xinjs",
  "version": "0.1.3",
  "description": "path-based state management for javascript",
  "main": "dist/index.js",
  "homepage": "https://xinjs.net",
  "keywords": [
    "javascript",
    "typescript",
    "bun",
    "front-end",
    "back-end"
  ],
  "scripts": {
    "start": "bun dev",
    "lint": "bun ts-standard --fix src/*.ts",
    "make": "rm -rf dist && bun test && bun tsc && ./build.command",
    "pack": "bun lint && bun run make && npm pack --pack-destination ~",
    "rerun": "./rerun.command",
    "test": "bun wiptest"
  },
  "author": "Tonio Loewald",
  "license": "BSD-3-CLAUSE",
  "devDependencies": {
    "@types/react": "^18.0.22",
    "bun-types": "^0.2.0",
    "react": "^18.2.0",
    "rollup": "^3.2.3",
    "ts-standard": "^12.0.1",
    "typescript": "^4.8.4",
    "uglify-js": "^3.1.7",
    "babylonjs": "^5.31.2",
    "babylonjs-gui": "^5.32.1",
    "babylonjs-loaders": "^5.32.1",
    "babylonjs-materials": "^5.32.1",
    "marked": "^4.2.2"
  },
  "files": [
    "/cdn",
    "/dist",
    "/src",
    "/LICENSE",
    "/README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/xinjs.git"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "cdn",
      "public",
      "**/*.js",
      "**/*.mjs",
      "**/*.d.ts"
    ]
  }
}
